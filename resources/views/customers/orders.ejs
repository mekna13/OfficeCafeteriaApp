<br>
<h1>Orders</h1>
<% if(messages.success) { %>
<div id="success-alert" role="alert">
    <p><%= messages.success %></p>
</div>
<% } %>
<br>
<table>
 <thead>
     <tr>
         <th>Orders</th>
         <th>Time</th>
     </tr>
 </thead>
 <tbody>
     <% if(orders.length) { %>
        <% orders.forEach((order) => { %>
            <tr>
                <td><a class="link" href="/customer/orders/<%= order._id %>"><%= order._id %></a></td>
                <td><%= moment(order.createdAt).format('hh:mm A') %></td>
            </tr>
        <% }) %>
     <% } else { %>
        <tr>
            <td>No orders found in DB</td>
        </tr>
     <% } %>
 </tbody>
 
 
</table>